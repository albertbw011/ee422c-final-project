<VBox alignment="TOP_CENTER" maxHeight="1080.0" maxWidth="1920.0" minHeight="720.0" minWidth="1280.0" prefHeight="900.0" prefWidth="1600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientController">
   <children>
      <Text fx:id="title" strokeType="OUTSIDE" strokeWidth="0.0" text="eHills">
         <font>
            <Font name="Segoe UI Bold" size="30.0" />
         </font>
      </Text>
      <ScrollPane fx:id="itemListPane" prefHeight="787.0" prefWidth="1590.0">
         <content>
            <VBox fx:id="itemListPaneVBox" prefHeight="818.0" prefWidth="1570.0" style="-fx-background-color: white;">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <opaqueInsets>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </opaqueInsets></VBox>
         </content>
      </ScrollPane>
      <HBox alignment="TOP_CENTER" prefHeight="35.0" prefWidth="1580.0">
         <children>
            <Button fx:id="logOutButton" mnemonicParsing="false" onAction="#logOutAction" onMouseEntered="#logOutMouseHover" onMouseExited="#logOutMouseExit" style="-fx-background-color: WHITE; -fx-border-color: BLACK; -fx-border-radius: 5;" text="Log Out">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
